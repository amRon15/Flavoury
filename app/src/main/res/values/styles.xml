<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="circleImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
    <style name="roundImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10dp</item>
    </style>

    <style name="AppTheme.PopUpOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="TitleTextSize" parent="@style/Base.TextAppearance.AppCompat.Title">
        <item name="android:textSize">40sp</item>
        <item name="android:fontFamily">@font/italiana</item>
    </style>

    <style name="tab_title" parent="@android:style/TextAppearance.Widget.TabWidget">
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:textColor">@color/secondaryText_color</item>
        <item name="android:textSize">16sp</item>
    </style>
</resources>