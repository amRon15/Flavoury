<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Setting_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:backgroundTint="#00FFFFFF"
    tools:context=".ui.Setting.SettingFragment">

    <ImageButton
        android:id="@+id/icon_backbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#00FFFFFF"
        android:contentDescription="backpage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_arrow_back_ios_new_24"
        app:tint="@color/mainText_color" />

    <TextView
        android:id="@+id/Setting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:text="Settings"
        android:textColor="@color/mainText_color"
        android:textSize="30dp"
        app:layout_constraintEnd_toEndOf="@+id/icon_backbutton"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/icon_backbutton"
        app:layout_constraintTop_toBottomOf="@+id/icon_backbutton" />

    <ImageButton
        android:id="@+id/setting_icon"
        android:layout_width="29dp"
        android:layout_height="29dp"
        android:layout_marginTop="32dp"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/Setting"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Setting"
        app:layout_constraintTop_toBottomOf="@+id/Setting"
        app:srcCompat="@drawable/baseline_account_circle_24" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="Account"
        android:textColor="@color/mainText_color"
        android:textSize="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/setting_icon"
        app:layout_constraintStart_toEndOf="@+id/setting_icon"
        app:layout_constraintTop_toTopOf="@+id/setting_icon"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/setting_line1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="8dp"
        android:background="@color/mainText_color"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="@+id/setting_icon"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/Setting_text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Edit profile"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/setting_line1"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toStartOf="@+id/setting_line1"
        app:layout_constraintTop_toBottomOf="@+id/setting_line1" />

    <ImageButton
        android:id="@+id/Setting_backbutton2"
        android:layout_width="22dp"
        android:layout_height="25dp"
        android:layout_marginStart="200dp"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/Setting_text1"
        app:layout_constraintStart_toEndOf="@+id/Setting_text1"
        app:layout_constraintTop_toTopOf="@+id/Setting_text1"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/baseline_arrow_forward_ios_24" />

    <TextView
        android:id="@+id/Setting_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Change password"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/Setting_text1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Setting_text1"
        app:layout_constraintTop_toBottomOf="@+id/Setting_text1" />

    <ImageButton
        android:id="@+id/Setting_backbutton3"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginTop="5dp"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/Setting_text2"
        app:layout_constraintEnd_toEndOf="@+id/Setting_backbutton2"
        app:layout_constraintStart_toStartOf="@+id/Setting_backbutton2"
        app:layout_constraintTop_toTopOf="@+id/Setting_text2"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/baseline_arrow_forward_ios_24" />

    <ImageView
        android:id="@+id/Setting_text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toEndOf="@+id/Setting_text2"
        app:layout_constraintHorizontal_bias="0.03"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Setting_text2"
        app:srcCompat="@drawable/baseline_account_circle_24" />

    <TextView
        android:id="@+id/Setting_text4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="8dp"
        android:text="Notifications"
        android:textColor="@color/mainText_color"
        android:textSize="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/Setting_text3"
        app:layout_constraintStart_toEndOf="@+id/Setting_text3"
        app:layout_constraintTop_toTopOf="@+id/Setting_text3" />

    <View
        android:id="@+id/Setting_line2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="8dp"
        android:background="@color/mainText_color"
        app:layout_constraintEnd_toEndOf="@+id/Setting_text4"
        app:layout_constraintHorizontal_bias="0.145"
        app:layout_constraintStart_toStartOf="@+id/Setting_text3"
        app:layout_constraintTop_toBottomOf="@+id/Setting_text4" />

    <TextView
        android:id="@+id/Setting_text5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Notifications"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/Setting_line2"
        app:layout_constraintHorizontal_bias="0.117"
        app:layout_constraintStart_toStartOf="@+id/Setting_line2"
        app:layout_constraintTop_toBottomOf="@+id/Setting_line2" />

    <com.google.android.material.switchmaterial.SwitchMaterial
        android:id="@+id/switch_notifications1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="168dp"
        app:layout_constraintBottom_toBottomOf="@+id/Setting_text5"
        app:layout_constraintStart_toEndOf="@+id/Setting_text5"
        app:layout_constraintTop_toTopOf="@+id/Setting_text5"
        app:layout_constraintVertical_bias="0.714" />

    <ImageButton
        android:id="@+id/Setting_backbutton4"
        android:layout_width="28dp"
        android:layout_height="30dp"
        android:layout_marginTop="32dp"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/Setting_text5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Setting_text5"
        app:layout_constraintTop_toBottomOf="@+id/Setting_text5"
        app:srcCompat="@drawable/baseline_notes_24" />

    <TextView
        android:id="@+id/Setting_text6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="More"
        android:textColor="@color/mainText_color"
        android:textSize="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/Setting_backbutton4"
        app:layout_constraintStart_toEndOf="@+id/Setting_backbutton4"
        app:layout_constraintTop_toTopOf="@+id/Setting_backbutton4"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/Setting_line3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="8dp"
        android:background="@color/mainText_color"
        app:layout_constraintEnd_toEndOf="@+id/Setting_text6"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="@+id/Setting_backbutton3"
        app:layout_constraintTop_toBottomOf="@+id/Setting_text6" />

    <TextView
        android:id="@+id/Setting_text7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Dark Mode"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Setting_line3" />

    <com.google.android.material.switchmaterial.SwitchMaterial
        android:id="@+id/switch_darkmode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="188dp"
        app:layout_constraintBottom_toBottomOf="@+id/Setting_text7"
        app:layout_constraintStart_toEndOf="@+id/Setting_text7"
        app:layout_constraintTop_toTopOf="@+id/Setting_text7"
        app:layout_constraintVertical_bias="0.428" />

    <TextView
        android:id="@+id/Setting_text8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Language"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/Setting_text7"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Setting_text7"
        app:layout_constraintTop_toBottomOf="@+id/Setting_text7" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Help"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/Setting_text8"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Setting_text8"
        app:layout_constraintTop_toBottomOf="@+id/Setting_text8" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Feedback"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/textView10"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="204dp"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/Setting_text8"
        app:layout_constraintStart_toEndOf="@+id/Setting_text8"
        app:layout_constraintTop_toTopOf="@+id/Setting_text8"
        app:layout_constraintVertical_bias="0.437"
        app:srcCompat="@drawable/baseline_arrow_forward_ios_24" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/textView10"
        app:layout_constraintEnd_toEndOf="@+id/imageButton6"
        app:layout_constraintStart_toStartOf="@+id/imageButton6"
        app:layout_constraintTop_toTopOf="@+id/textView10"
        app:layout_constraintVertical_bias="0.687"
        app:srcCompat="@drawable/baseline_arrow_forward_ios_24" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/textView11"
        app:layout_constraintEnd_toEndOf="@+id/imageButton7"
        app:layout_constraintStart_toStartOf="@+id/imageButton7"
        app:layout_constraintTop_toTopOf="@+id/textView11"
        app:srcCompat="@drawable/baseline_arrow_forward_ios_24" />

    <com.google.android.material.button.MaterialButton
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="30dp"
        android:fontFamily="@font/inter_semibold"
        android:padding="25dp"
        android:text="Logout"
        android:textColor="@color/Secondary_color"
        android:textSize="15dp"
        app:backgroundTint="#99FFFFFF"
        app:cornerRadius="35dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.416"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Setting_line3"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>